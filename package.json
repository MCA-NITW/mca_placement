{
  "name": "mca_placement",
  "version": "1.0.0",
  "description": "MCA Placement Portal",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write .",
    "frontend-start": "cd client && npm start",
    "frontent-build": "cd client && npm run build",
    "frontend-install": "cd client && npm install",
    "backend-start": "cd server && npm start",
    "backend-install": "cd server && npm install",
    "start": "concurrently \"npm run backend-start\" \"npm run frontend-start\"",
    "build": "concurrently \"npm run backend-install\" \"npm run frontend-install\" \"npm run frontent-build\""
  },
  "author": "Sagar Gupta",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^3.1.1"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
